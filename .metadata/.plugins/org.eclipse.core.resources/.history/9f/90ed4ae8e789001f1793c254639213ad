package oop.constructorcalling;

public class ConstructorCalling {
	
	public ConstructorCalling() {
		System.out.println("i  am default constructor");
		}

	public ConstructorCalling(int a) {
		this();
		System.out.println("i am one parameterize constructor:"+a);
	}
	
	public ConstructorCalling(int a, int b) {
		this(a);
		System.out.println("i am 2  parameterize constructor:"+(a+b));
	}
	public ConstructorCalling(int a, int b,int c) {
		this (a,b);
		System.out.println("i am 3  parameterize constructor:"+(a+b+c));
		
		
	}

}
